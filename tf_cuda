conda activate tf_env

pip install tensorflow[and-cuda]==2.19.0
pip install wfdb biosppy neurokit2 scipy numpy pandas matplotlib seaborn \
            scikit-learn imbalanced-learn tqdm optuna \
            tensorboard joblib h5py pyarrow \
            audiomentations tsaug \
            shap mlflow

